{
    "name": "vlabs/adrenalina",
    "type": "project",
    "description": "The Laravel Framework.",
    "license": "proprietary",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/pedro-santiago/nova.git",
            "options":  {
                "http": {
                    "header": [
                        "API-TOKEN: 2421aa66bdce92f6ed89d6cb4394506c542a8812"
                    ]
                }
            }
        }
    ],
    "require": {
        "php": "^7.1.3",
        "fideloper/proxy": "^4.0",
        "jobcerto/nova-grid": "^1.0",
        "laravel/framework": "6.0.*",
        "laravel/nova": "2.1.1",
        "laravel/passport": "^7.4",
        "laravel/tinker": "^1.0",
        "stonkeep/cpf-cnpj": "^0.0.1",
        "wemersonrv/input-mask": "^0.1.2"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^7.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "github-oauth": {
            "github.com": "2421aa66bdce92f6ed89d6cb4394506c542a8812"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },

    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },

}
