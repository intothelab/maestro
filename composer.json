{
    "name": "vlabs/adrenalina",
    "type": "project",
    "description": "The Laravel Framework.",
    "license": "proprietary",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://pedrosantiago@bitbucket.org/pedrosantiago/nova.git"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-json": "*",
        "ext-simplexml": "*",
        "anaseqal/nova-sidebar-icons": "^0.0.3",
        "fideloper/proxy": "^4.0",
        "geekcom/validator-docs": "^2.0",
        "geocoder-php/here-provider": "^0.3.0",
        "grimzy/laravel-mysql-spatial": "^2.2",
        "jenssegers/mongodb": "^3.6",
        "jobcerto/nova-grid": "^1.0",
        "laravel/framework": "6.0.*",
        "laravel/nova": "^2.6",
        "laravel/passport": "^7.4",
        "laravel/tinker": "^1.0",
        "outhebox/nova-hidden-field": "^1.0",
        "ovac/idoc": "^1.2",
        "predis/predis": "^1.1",
        "sentry/sentry-laravel": "^1.3",
        "stonkeep/cpf-cnpj": "^0.0.1",
        "toin0u/geocoder-laravel": "^4.2",
        "wemersonrv/input-mask": "^0.1.2",
        "whitecube/nova-flexible-content": "^0.1.12"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.6",
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^7.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "github-oauth": {

        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
